
@model Employee

<h2>@Model.LastName, @Model.FirstName @Model.MiddleName</h2>

<div>
    Employee Id: @Model.EmployeeId
</div>
<div>
    Email: @Model.EmailAlias@@CompanyEmail.com
</div>
<div>
    Office: @Model.Office
</div>
<div>
    Region: @Model.Region
</div>

@if(ViewBag.Message != null)
{
    <div class="alert alert-info">@ViewBag.Message</div>
}

<a asp-controller="Employee" asp-action="List" class="btn btn-outline-primary">Return to All Employees'</a>